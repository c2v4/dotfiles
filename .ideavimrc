set surround
set multiple-cursors
set incsearch
set ignorecase
set clipboard=unnamed

noremap <Up> <Nop>
noremap <Down> <Nop>
noremap <Left> <Nop>
noremap <Right> <Nop>

set clipbord+=ideaput

set showcmd
set showmatch
set smartcase
set incsearch
set autowrite
set mouse=a
set number relativenumber

set highlightedyank

noremap p "0p
noremap P "0P
for s:i in ['"','*','+','-','.',':','%','/','=','1','2','3','4','5','6','7','8','9','a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z']
    execute 'noremap "'.s:i.'p "'.s:i.'p'
    execute 'noremap "'.s:i.'P "'.s:i.'P'
endfor